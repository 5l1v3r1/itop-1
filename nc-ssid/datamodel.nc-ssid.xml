<?xml version="1.0" encoding="UTF-8"?>
<itop_design xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0">
<!--Definicion de nueva clase SSID -->
  <classes>
    <class id="SSID" _delta="define">
      <parent>ConnectableCI</parent>
      <properties>
        <category>bizmodel,searchable,configmgmt</category>
        <abstract>false</abstract>
        <key_type>autoincrement</key_type>
        <db_table>ssid</db_table>
        <db_key_field>id</db_key_field>
        <db_final_class_field/>
        <naming>
          <attributes>
            <attribute id="name"/>
          </attributes>
        </naming>
        <display_template/>
        <icon>images/ssid.png</icon>
        <reconciliation>
          <attributes>
            <attribute id="name"/>
            <attribute id="org_id"/>
            <attribute id="organization_name"/>
          </attributes>
        </reconciliation>
      </properties>
      <fields>
        <field id="vlan_tag" xsi:type="AttributeString">
          <sql>vlan_tag</sql>
          <is_null_allowed>true</is_null_allowed>
        </field>
        <field id="security_type" xsi:type="AttributeEnum" >
          <values>
            <value>Open</value>
            <value>WEP</value>
	          <value>WAP-PSK</value>
            <value>WAP2-PSK</value>
            <value>WAP-Ent</value>
            <value>WAP2-Ent</value>
          </values>
          <sql>security_type</sql>
          <is_null_allowed>true</is_null_allowed>
        </field>
        <field id="encryption" xsi:type="AttributeEnum">
              <values>
                <value>TKIP</value>
                <value>AES</value>
              </values>
              <sql>encryption</sql>
              <default_value/>
              <is_null_allowed>true</is_null_allowed>
              <display_style>radio_horizontal</display_style>
        </field>
        <field id="authentication_method" xsi:type="AttributeEnum" >
          <values>
            <value>Ninguno</value>
            <value>EAP-PEAP</value>
	          <value>EAP-TSL</value>
            <value>EAP-TTLSK</value>
            <value>MSCHAPv2</value>
          </values>
          <sql>authentication_method</sql>
          <is_null_allowed>true</is_null_allowed>
        </field>
        <field id="broadcast" xsi:type="AttributeEnum">
              <values>
                <value>Si</value>
                <value>No</value>
              </values>
              <sql>broadcast</sql>
              <default_value/>
              <is_null_allowed>true</is_null_allowed>
              <display_style>radio_horizontal</display_style>
        </field>
      </fields>
      <methods/>
      <presentation>
        <details>
          <items>
            <item id="name">
              <rank>10</rank>
            </item>
            <item id="org_id">
              <rank>10</rank>
            </item>
            <item id="vlan_tag">
              <rank>20</rank>
            </item>
            <item id="security_type">
              <rank>30</rank>
            </item>
            <item id="encryption">
              <rank>40</rank>
            </item>
            <item id="authentication_method">
              <rank>50</rank>
            </item>
            <item id="broadcast">
              <rank>50</rank>
            </item>
          </items>
        </details>
        <search>
          <items>
            <item id="name">
              <rank>10</rank>
            </item>
            <item id="vlan_tag">
              <rank>20</rank>
            </item>
            <item id="security_type">
              <rank>30</rank>
            </item>
            <item id="encryption">
              <rank>40</rank>
            </item>
          </items>
        </search>
        <list>
          <items>
            <item id="org_id">
              <rank>10</rank>
            </item>
            <item id="name">
              <rank>20</rank>
            </item>
            <item id="vlan_tag">
              <rank>30</rank>
            </item>
            <item id="security_type">
              <rank>40</rank>
            </item>
            <item id="encryption">
              <rank>50</rank>
            </item>
            <item id="broadcast">
              <rank>50</rank>
            </item>
          </items>
        </list>
      </presentation>
    </class>
  </classes>
  <menus>
  <menu id="ConfigManagementOverview" xsi:type="DashboardMenuNode" _delta="must_exist">
    <definition>
      <cells>
        <cell id="2" _delta="must_exist">
          <dashlets>
            <dashlet id="400" xsi:type="DashletBadge" _delta="define">
              <rank>40</rank>
              <class>SSID</class>
            </dashlet>
          </dashlets>
        </cell>
      </cells>
    </definition>
  </menu>
</menus>
</itop_design>
